    <script src="/SlickGrid-master/lib/jquery-1.7.min.js"></script>
    <script src="/SlickGrid-master/lib/jquery.event.drag-2.2.js"></script>

    <script src="/SlickGrid-master/slick.dataview.js"></script>

    <script src="/SlickGrid-master/slick.core.js"></script>
    <script src="/SlickGrid-master/slick.grid.js"></script>

    <script type="text/javascript">
      var data_view = new Slick.Data.DataView();

      var columns = [
        {id : "cash_id", name : "순번", field : "cash_id"},
        {id : "cash_name", name : "이름", field : "cash_name"},
        {id : "cash_value", name : "값", field : "cash_value"},
        {id : "cash_kind", name : "종류", field : "cash_kind"}
      ];


      var data = [];
      var options = {
        enableCellNavgation : true,
        enableColumnReorder : false
      };
      var gird = new Slick.Grid("#myGrid", data_view, columns, options);

      function AddRow(){
        data = {
          "cash_id" : 1,
          "cash_name" : 2,
          "cash_value" : 3,
          "cash_kind" : 4
        };
        data_view.insertItem(0,data);
      };


    </script>


