  <body>

    <div style="float : right;">
      <button class="w3-btn" type="button" name="button" onclick="AddRow()">+</button>
      <button class="w3-btn" type="button" name="button">-</button>
    </div>
    <table width="100%">
      <tr height="100%">
        <td>
          <div id="myGrid" style="width:600px;height:500px;"></div>
        </td>
      </tr>
    </table>



